{
  "name": "autocore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "pnpm exec nodemon index.js",
    "start": "node index.js",
    "build": "pnpm exec esbuild index.js --bundle --platform=node --outfile=dist/index.js",
    "prod": "pnpm run build && pnpm run deploy-commands && node dist/index.js",
    "deploy-commands": "pnpm exec node src/deploy-commands.js",
    "format": "pnpm exec prettier --write .",
    "clean": "rm -rf node_modules dist && pnpm install",
    "turbo": "pnpm exec turbo run"
  },
  "keywords": [
    "discord",
    "bot",
    "autocore"
  ],
  "author": "nocacode",
  "license": "ISC",
  "dependencies": {
    "cli-spinners": "^3.2.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2"
  }
}
