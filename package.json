{
  "name": "autocore",
  "version": "1.0.0",
  "description": "Discord.js Bot",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node dist/index.js",
    "build": "pnpm exec esbuild index.js src/**/*.js --bundle --platform=node --format=esm --outdir=dist --external:node:* --external:discord.js --external:dotenv --external:cron",
    "prod": "pnpm run build && pnpm run deploy-commands && pnpm run start",
    "deploy-commands": "node dist/src/deploy-commands.js",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "clean": "rm -rf node_modules dist && pnpm install",
    "rebuild": "pnpm run clean && pnpm install && pnpm run build"
  },
  "keywords": [
    "discord",
    "bot",
    "autocore"
  ],
  "author": "nocacode",
  "license": "ISC",
  "dependencies": {
    "cli-spinners": "^3.2.0",
    "cron": "^4.1.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "esbuild": "^0.25.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1"
  }
}
